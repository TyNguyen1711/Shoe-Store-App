<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape>
                <size
                    android:width="4dp"
                    android:height="100dp" />
                <solid android:color="@color/black" />
            </shape>
        </rotate>
    </item>

    <item>
        <rotate
            android:fromDegrees="-45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape>
                <size
                    android:width="4dp"
                    android:height="100dp" />
                <solid android:color="@color/black" />
            </shape>
        </rotate>
    </item>

</layer-list>
